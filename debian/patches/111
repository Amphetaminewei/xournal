+$as_echo "$ac_cv_search_XUngrabButton" >&6; }
+ac_res=$ac_cv_search_XUngrabButton
+if test "$ac_res" != no; then :
+  test "$ac_res" = "none required" || LIBS="$ac_res $LIBS"
+  
+fi
