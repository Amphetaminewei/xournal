+libttsubset.a: $(libttsubset_a_OBJECTS) $(libttsubset_a_DEPENDENCIES) $(EXTRA_libttsubset_a_DEPENDENCIES) 
+	$(AM_V_at)-rm -f libttsubset.a
+	$(AM_V_AR)$(libttsubset_a_AR) libttsubset.a $(libttsubset_a_OBJECTS) $(libttsubset_a_LIBADD)
+	$(AM_V_at)$(RANLIB) libttsubset.a
