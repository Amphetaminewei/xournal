+CATALOGS=' ca.gmo ca@valencia.gmo cs.gmo de.gmo es.gmo fr.gmo it.gmo ja.gmo nl.gmo pl.gmo pt_BR.gmo ru.gmo zh_CN.gmo zh_TW.gmo'
+CATOBJEXT='.gmo'
