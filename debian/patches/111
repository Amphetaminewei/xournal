+LTLIBOBJS = 
+MAINT = #
+MAKEINFO = ${SHELL} /home/qinxuchu/Src/xournal/xournal-0.4.8.2016/missing makeinfo
+MKDIR_P = /usr/bin/mkdir -p
+MKINSTALLDIRS = ./mkinstalldirs
+MSGFMT = /usr/bin/msgfmt
+MSGFMT_OPTS = -c
+OBJEXT = o
+PACKAGE = xournal
+PACKAGE_BUGREPORT = 
+PACKAGE_CFLAGS = -pthread -I/usr/include/libgnomecanvas-2.0 -I/usr/include/gail-1.0 -I/usr/include/libart-2.0 -I/usr/include/gtk-2.0 -I/usr/lib/x86_64-linux-gnu/gtk-2.0/include -I/usr/include/pango-1.0 -I/usr/include/atk-1.0 -I/usr/include/gdk-pixbuf-2.0 -I/usr/include/libmount -I/usr/include/blkid -I/usr/include/poppler/glib -I/usr/include/poppler -I/usr/include/pango-1.0 -I/usr/include/fribidi -I/usr/include/cairo -I/usr/include/pixman-1 -I/usr/include/harfbuzz -I/usr/include/glib-2.0 -I/usr/lib/x86_64-linux-gnu/glib-2.0/include -I/usr/include/uuid -I/usr/include/freetype2 -I/usr/include/libpng16
+PACKAGE_LIBS = -lgnomecanvas-2 -lart_lgpl_2 -lgtk-x11-2.0 -lgdk-x11-2.0 -lpangocairo-1.0 -latk-1.0 -lgdk_pixbuf-2.0 -lgio-2.0 -lpoppler-glib -lcairo -lpangoft2-1.0 -lpango-1.0 -lgobject-2.0 -lglib-2.0 -lharfbuzz -lfontconfig -lfreetype
+PACKAGE_NAME = 
+PACKAGE_STRING = 
+PACKAGE_TARNAME = 
+PACKAGE_URL = 
+PACKAGE_VERSION = 
+PATH_SEPARATOR = :
+PKG_CONFIG = /usr/bin/pkg-config
+PKG_CONFIG_LIBDIR = 
+PKG_CONFIG_PATH = 
+POFILES =  ca.po ca@valencia.po cs.po de.po es.po fr.po it.po ja.po nl.po pl.po pt_BR.po ru.po zh_CN.po zh_TW.po
+POSUB = po
+PO_IN_DATADIR_FALSE = 
+PO_IN_DATADIR_TRUE = 
+RANLIB = ranlib
+SET_MAKE = 
+SHELL = /bin/bash
+STRIP = 
+USE_NLS = yes
+VERSION = 0.4.8.2016
+XGETTEXT = /usr/bin/xgettext
+abs_builddir = /home/qinxuchu/Src/xournal/xournal-0.4.8.2016
+abs_srcdir = /home/qinxuchu/Src/xournal/xournal-0.4.8.2016
+abs_top_builddir = /home/qinxuchu/Src/xournal/xournal-0.4.8.2016
+abs_top_srcdir = /home/qinxuchu/Src/xournal/xournal-0.4.8.2016
+ac_ct_AR = ar
+ac_ct_CC = gcc
+am__include = include
+am__leading_dot = .
+am__quote = 
+am__tar = $${TAR-tar} chof - "$$tardir"
+am__untar = $${TAR-tar} xf -
+bindir = ${exec_prefix}/bin
+build = x86_64-pc-linux-gnu
+build_alias = x86_64-linux-gnu
+build_cpu = x86_64
